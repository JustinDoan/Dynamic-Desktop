<Window x:Class="VirtuaDesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VirtuaDesktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="204.984" Width="473.754">
    <Grid Margin="0,0,0,177">
        <Button x:Name="button" Content="Edit" HorizontalAlignment="Left" Height="28" Margin="252,137,0,-165" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button1" Content="Remove" HorizontalAlignment="Left" Height="28" Margin="371,137,0,-165" VerticalAlignment="Top" Width="75"/>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="119" Margin="1,1,0,-120" VerticalAlignment="Top" Width="445">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Desktops" Width="150" DisplayMemberBinding="{Binding Desktop}"/>
                    <GridViewColumn Header="Favorite" DisplayMemberBinding="{Binding Favorite}"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
